g_id` = `Organization_org_id`.`id`\n   ) ON `PDU`.`id` = `PDU_FunctionalCI`.`id`\n WHERE (((COALESCE((`Brand_brand_id_Typology`.`finalclass` IN (\'Brand\')), 1) AND COALESCE((`Model_model_id_Typology`.`finalclass` IN (\'Model\')), 1)) AND COALESCE((`Rack_rack_id_PhysicalDevice`.`finalclass` IN (\'Rack\')), 1)) AND COALESCE((`PowerConnection_powerstart_id_PhysicalDevice`.`finalclass` IN (\'PowerSource\', \'PDU\', \'PowerConnection\')), 1))
