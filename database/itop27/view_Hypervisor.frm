JOIN \n         `functionalci` AS `Server_server_id_FunctionalCI`\n       ON `Server_server_id_PhysicalDevice`.`id` = `Server_server_id_FunctionalCI`.`id`\n   ) ON `Hypervisor`.`server_id` = `Server_server_id_PhysicalDevice`.`id`\n   INNER JOIN \n      `virtualdevice` AS `Hypervisor_VirtualDevice`\n    ON `Hypervisor`.`id` = `Hypervisor_VirtualDevice`.`id`\n   INNER JOIN (\n      `functionalci` AS `Hypervisor_FunctionalCI` \n      INNER JOIN \n         `organization` AS `Organization_org_id`\n       ON `Hypervisor_FunctionalCI`.`org_id` = `Organization_org_id`.`id`\n   ) ON `Hypervisor`.`id` = `Hypervisor_FunctionalCI`.`id`\n WHERE (COALESCE((`Farm_farm_id_VirtualDevice`.`finalclass` IN (\'Farm\')), 1) AND COALESCE((`Server_server_id_PhysicalDevice`.`finalclass` IN (\'Server\')), 1))
