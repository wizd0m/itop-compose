`location` AS `Location_location_id`\n       ON `Rack_PhysicalDevice`.`location_id` = `Location_location_id`.`id`\n      LEFT JOIN \n         `typology` AS `Brand_brand_id_Typology`\n       ON `Rack_PhysicalDevice`.`brand_id` = `Brand_brand_id_Typology`.`id`\n      LEFT JOIN \n         `typology` AS `Model_model_id_Typology`\n       ON `Rack_PhysicalDevice`.`model_id` = `Model_model_id_Typology`.`id`\n   ) ON `Rack`.`id` = `Rack_PhysicalDevice`.`id`\n   INNER JOIN (\n      `functionalci` AS `Rack_FunctionalCI` \n      INNER JOIN \n         `organization` AS `Organization_org_id`\n       ON `Rack_FunctionalCI`.`org_id` = `Organization_org_id`.`id`\n   ) ON `Rack`.`id` = `Rack_FunctionalCI`.`id`\n WHERE (COALESCE((`Brand_brand_id_Typology`.`finalclass` IN (\'Brand\')), 1) AND COALESCE((`Model_model_id_Typology`.`finalclass` IN (\'Model\')), 1))
